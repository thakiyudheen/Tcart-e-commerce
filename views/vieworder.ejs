<%- include("./layout/layout")%>
<%- include("./partials/partials")%>
<div class="container">
    <div class="row mt-2 p-5">
        <div class="col-md">
            <table class="table table-hover table-light ">
                <thead>
                  <tr>
                    <th scope="col">No</th>
                    
            
                    <th scope="col">Date  </th>
                    <th scope="col">Name  </th>
                    <th scope="col">Address</th>
                    <th scope="col">Mobile </th>
                    <th scope="col">Amount</th>
                    <th scope="col">Payment </th>
                    <th scope="col">status </th>
            
                    
                    
                    
                  </tr>
                </thead>
                <tbody>
                  <% orderlist.forEach((item,index) => { %>
                    
                    <tr>
                      <th scope="row"><%= index+1 %></th>
                      
                      <td><%= item.deliverydetails.date %></td>
                      <td><%= item.deliverydetails.Name %></td>
                      <td><%= item.deliverydetails.adress %></td>
                      <td><%= item.deliverydetails.phone %></td>
                      <td><%= item.deliverydetails.totel %></td>
                      <td><%= item.deliverydetails.delivery %></td>
                      <td><%= item.deliverydetails.status %></td>
                      <td><a class="btn btn-primary" href="/view-pro?orderid=<%= item._id %>">view products</a></td>


                      
                    </tr>
                  <% }) %>%>
                  
                  
             
                </tbody>
              </table>

        </div>
    </div>

  </div>
